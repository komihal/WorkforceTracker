            <ActionContent
               title = "Copy Hermes dSYM to Archive"
               scriptText = "set -euo pipefail

APP_DSYMS_DIR=&quot;${ARCHIVE_PATH}/dSYMs&quot;
mkdir -p &quot;${APP_DSYMS_DIR}&quot;

# Путь к Hermes внутри Pods (prebuilt xcframework)
HERMES_BIN=$(find &quot;${SRCROOT}/Pods/hermes-engine&quot; \
  -type f -path &quot;*hermes.xcframework/ios-arm64/hermes.framework/hermes&quot; | head -n1)

# Если внутри XCFramework уже есть dSYM — просто копируем его; иначе генерим через dsymutil
HERMES_DSYM_SRC=$(find &quot;${SRCROOT}/Pods/hermes-engine&quot; \
  -type d -path &quot;*hermes.xcframework/dSYMs/ios-arm64/hermes.framework.dSYM&quot; | head -n1 || true)

if [ -n &quot;${HERMES_DSYM_SRC:-}&quot; ] &amp;&amp; [ -d &quot;$HERMES_DSYM_SRC&quot; ]; then
  echo &quot;Копируем существующий dSYM для Hermes...&quot;
  rsync -a &quot;$HERMES_DSYM_SRC&quot; &quot;${APP_DSYMS_DIR}/&quot;
elif [ -n &quot;${HERMES_BIN:-}&quot; ] &amp;&amp; [ -f &quot;$HERMES_BIN&quot; ]; then
  echo &quot;Генерируем dSYM для Hermes из бинаря...&quot;
  /usr/bin/dsymutil &quot;$HERMES_BIN&quot; -o &quot;${APP_DSYMS_DIR}/hermes.framework.dSYM&quot; || true
fi

# Показать UUID для проверки (не обязательно)
if [ -d &quot;${APP_DSYMS_DIR}/hermes.framework.dSYM&quot; ]; then
  echo &quot;UUID dSYM для Hermes:&quot;
  /usr/bin/dwarfdump --uuid &quot;${APP_DSYMS_DIR}/hermes.framework.dSYM&quot; || true
fi

echo &quot;dSYM для Hermes добавлен в архив: ${APP_DSYMS_DIR}/hermes.framework.dSYM&quot;">
               <EnvironmentBuildable>
